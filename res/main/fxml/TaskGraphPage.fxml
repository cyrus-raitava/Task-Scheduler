<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se306group8.scheduleoptimizer.visualisation.controller.TaskGraphPageController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <left>
      			<VBox fx:id="processors" alignment="CENTER" spacing="10" BorderPane.alignment="CENTER" />
         </left>
         <center>
            <VBox fx:id="tasks" alignment="CENTER" spacing="10" BorderPane.alignment="CENTER">
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
